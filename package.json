{
  "name": "plst2",
  "version": "0.1.0",
  "description": "playlist app",
  "type": "module",
  "module": "main.ts",
  "repository": "https://github.com/btmxh/plst2",
  "author": "btmxh",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "devDependencies": {
    "@parcel/transformer-pug": "2.9.3",
    "@parcel/transformer-sass": "2.9.3",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "parcel": "^2.9.3"
  },
  "resolutions": {
    "cosmiconfig": "8.2.0"
  },
  "scripts": {
    "start-watch": "nodemon --esm main.ts",
    "build-watch": "parcel watch public/index.pug public/404.pug",
    "dev": "concurrently --kill-others \"yarn start-watch\" \"yarn build-watch\"",
    "build": "parcel build public/index.pug public/404.pug",
    "start": "yarn build && ts-node --esm main.ts"
  },
  "targets": {
    "module": false
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
